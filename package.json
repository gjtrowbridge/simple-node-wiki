{
  "name": "basic-wiki",
  "version": "0.0.0",
  "description": "A basic wiki",
  "main": "server.js",
  "engines": {
    "node": "8.9.3"
  },
  "scripts": {
    "build": "npm run build-css & npm run build-js",
    "build-css": "stylus --include-css client/src/ --out client/public/assets/styles",
    "build-js": "npx webpack",
    "db:migrate": "sequelize db:migrate",
    "postinstall": "npm run build & npm run db:migrate",
    "start": "node server.js",
    "test:server": "NODE_ENV=test bash -c 'sequelize db:migrate:undo:all && sequelize db:migrate && tape tests/server/**/test_*.js | tap-spec || true'",
    "watch": "npm run watch-css & npm run watch-js & nodemon server",
    "watch-css": "stylus -w --include-css client/src/ --out client/public/assets/styles",
    "watch-js": "npx webpack-dev-server"
  },
  "keywords": [
    "wiki"
  ],
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ]
  },
  "author": "Greg Trowbridge",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.12.3",
    "express": "^4.12.3",
    "flux": "^2.0.3",
    "highlight.js": "^8.9.1",
    "history": "^4.9.0",
    "jsonwebtoken": "^8.3.0",
    "keymirror": "^0.1.1",
    "marked": "^0.3.19",
    "node-fetch": "^2.2.0",
    "normalize.css": "^3.0.3",
    "object-assign": "^3.0.0",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "passport-jwt": "^4.0.0",
    "pg": ">=6.4.2",
    "pg-hstore": "^2.3.2",
    "pg-native": "^3.0.0",
    "prop-types": "^15.7.2",
    "q": "^1.2.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router": "^0.13.3",
    "react-router-dom": "^5.0.0",
    "sequelize": ">=5.3.0",
    "sqlite3": "^3.1.13",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "acorn": "^6.1.1",
    "babel-loader": "^8.0.5",
    "nodemon": "^1.3.7",
    "sequelize-cli": "^4.0.0",
    "stylus": "^0.51.1",
    "tap-spec": "^5.0.0",
    "tape": "^4.10.1",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1"
  }
}
