{
  "name": "basic-wiki",
  "version": "0.0.0",
  "description": "A basic wiki",
  "main": "server.js",
  "engines": {
    "node": "8.9.3"
  },
  "scripts": {
    "build": "npx webpack",
    "db:migrate": "sequelize db:migrate",
    "postinstall": "npm run build & npm run db:migrate",
    "reset-test-db": "NODE_ENV=test bash -c 'sequelize db:migrate:undo:all && sequelize db:migrate'",
    "start": "node server.js",
    "start-in-test-env": "NODE_ENV=test bash -c 'node server.js'",
    "test:server": "npm run reset-test-db && npm run test:server:fast",
    "test:server:fast": "NODE_ENV=test bash -c 'tape `find tests/server/ -name test_*.js` | tap-spec || true'",
    "watch": "npm run watch-js & nodemon server",
    "watch-js": "npx webpack-dev-server"
  },
  "keywords": [
    "wiki"
  ],
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ]
  },
  "author": "Greg Trowbridge",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.12.3",
    "codemirror": "^5.47.0",
    "debounce": "^1.2.0",
    "express": "^4.17.3",
    "flux": "^2.0.3",
    "history": "^4.9.0",
    "jsonwebtoken": "^8.3.0",
    "keymirror": "^0.1.1",
    "marked": "^0.6.2",
    "node-fetch": "^2.2.0",
    "object-assign": "^3.0.0",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "passport-jwt": "^4.0.0",
    "pg": ">=6.4.2",
    "pg-hstore": "^2.3.2",
    "pg-native": "^3.0.0",
    "prismjs": "^1.16.0",
    "prop-types": "^15.7.2",
    "q": "^1.2.1",
    "react": "^16.8.6",
    "react-codemirror2": "^6.0.0",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "sequelize": ">=5.8.6",
    "sqlite3": "^3.1.13",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "acorn": "^6.1.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-prismjs": "^1.0.2",
    "css-loader": "^2.1.1",
    "node-sass": "^4.12.0",
    "nodemon": "^1.3.7",
    "sass-loader": "^7.1.0",
    "sequelize-cli": "^4.0.0",
    "style-loader": "^0.23.1",
    "supertest": "^4.0.2",
    "tap-spec": "^5.0.0",
    "tape": "^4.10.1",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1"
  }
}
